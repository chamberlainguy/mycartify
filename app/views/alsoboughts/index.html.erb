<p id="notice"><%= notice %></p>

<h1 class="heading">Listing (Also-Boughts)</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alsoboughts.each do |alsobought| %>
      <tr>
        <td><%= alsobought.product_id %></td>
        <td><%= link_to 'Show', alsobought %></td>
        <td><%= link_to 'Edit', edit_alsobought_path(alsobought) %></td>
        <td><%= link_to 'Destroy', alsobought, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alsobought', new_alsobought_path %>
